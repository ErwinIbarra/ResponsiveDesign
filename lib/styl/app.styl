@import './normalize.styl'

a
	text-decoration: none;
	color: inherit;
	
body
	font-size: 16px;
	color:gray;
.Contenedor
	width: 90%;
	margin: 0 auto;
	max-width:900px;
.Header
	&-nav
		ul
			margin: 0;
			padding:0;
			li
				list-style:none;
				text-align:center;
				padding: 1em 0;
	&-menu
		text-align:center;
	&-logo
		padding: 1.5em 0;
		border-top:5px solid black;
		border-bottom:5px solid black;
		figure
			margin:0 auto;
			width: 2em;
			height:2em;
			background:red;
			